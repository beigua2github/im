<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.starsea.im.biz.dao.EnergyAreaDao">


    <select id="queryEnergyArea"  resultType="EnergyAreaEntity">
        select * from test.EnergyArea
    </select>

    <select id="queryEnergyAreaById"  resultType="EnergyAreaEntity">
        select * from test.EnergyArea WHERE ID = #{id}
    </select>

</mapper>